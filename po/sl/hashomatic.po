#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Ta datoteka je bila podpisana skladno z VS-NfD s ključem <a href=\"%1\">%2</"
"a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Ta datoteka je bila podpisana s ključem <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Ključne podrobnosti niso na voljo."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "%1 je to datoteko podpisal kot skladno z VS-NfD."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "To datoteko je podpisal %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">ključ</a> je bil preklican."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">ključ</a> je potekel."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"Podpis je veljaven, vendar veljavnost <a href=\"%1\">ključa</a> ni znana."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr ""
"Podpis je veljaven in <a href=\"%1\">ključ</a> je malo zaupanja vreden."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr ""
"Podpis je veljaven in <a href=\"%1\">ključ</a> je v celoti zaupanja vreden."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"Podpis je veljaven in <a href=\"%1\">ključ</a> je končno zaupanja vreden."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr ""
"Podpis je veljaven, vendar <a href=\"%1\">ključ</a> ni vreden zaupanja."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "Podpis je neveljaven."

#: hashhelper.cpp:159
#, kde-format
msgctxt "list separator"
