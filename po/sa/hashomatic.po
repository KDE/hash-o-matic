#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"एषा सञ्चिका <a href=\"%1\">%2</a> कीलस्य उपयोगेन VS-NfD अनुरूपं हस्ताक्षरिता अस्ति ।"

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "एषा सञ्चिका <a href=\"%1\">%2</a> कीलस्य उपयोगेन हस्ताक्षरिता अस्ति ।"

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "मुख्यविवरणं उपलब्धं नास्ति।"

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "एषा सञ्चिका %1 द्वारा VS-NfD अनुरूपं हस्ताक्षरिता अस्ति ।"

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "एषा सञ्चिका %1 इत्यनेन हस्ताक्षरिता अस्ति ।"

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">कुञ्जी</a> निरस्तं जातम्।"

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">कीलकस्य</a> अवधिः समाप्तः अभवत् ।"

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr "हस्ताक्षरं वैधम् अस्ति, परन्तु <a href=\"%1\">कीलस्य</a> वैधता अज्ञाता अस्ति ।"

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "हस्ताक्षरं वैधं भवति तथा च <a href=\"%1\">कुञ्जी</a> सीमान्तरूपेण विश्वसिति।"

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "हस्ताक्षरं वैधं भवति, <a href=\"%1\">कुञ्जी</a> च पूर्णतया विश्वसिति।"

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr "हस्ताक्षरं वैधं भवति, <a href=\"%1\">कुञ्जी</a> च अन्ततः विश्वसिति।"

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "हस्ताक्षरं वैधं भवति, परन्तु <a href=\"%1\">कुञ्जी</a> अविश्वसनीयम् अस्ति ।"

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "हस्ताक्षरम् असिद्धम् अस्ति।"

