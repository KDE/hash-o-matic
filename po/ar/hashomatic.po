#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "تفاصيل المفتاح غير متوفّرة."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "هذا الملف موقَّع مطابقًا لـ VS-NfD من قِبَل %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "هذا الملف موقَّع من قِبَل %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "المفتاح <a href=\"%1\">أُلغي</a>."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "المفتاح <a href=\"%1\">انتهت صلاحيته</a>."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr "التوقيع صالح، لكن صلاحية المفتاح <a href=\"%1\"></a> غير معلومة."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "التوقيع صالح والمفتاح <a href=\"%1\"></a> موثوق به هامشيًا."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "التوقيع صالح والمفتاح <a href=\"%1\"></a> موثوق به كليًا."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr "التوقيع صالح والمفتاح <a href=\"%1\"></a> موثوق به تمامًا."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "التوقيع صالح، لكن المفتاح <a href=\"%1\"></a> غير موثوق به."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "التوقيع غير صالح."

