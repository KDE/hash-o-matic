#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Šī datne ir parakstīta kā „VS-NfD“ saderīga, izmantojot atslēgu <a href="
"\"%1\">%2</a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Šī datne ir parakstīta, izmantojot atslēgu <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Informācija par atslēgu nav pieejama."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Šo datni kā „VS-NfD“ saderīgu parakstīja ar %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Šo datni parakstīja %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">Atslēga</a> ir atsaukta."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">Atslēgas</a> termiņš ir beidzies."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"Paraksts ir derīgs, bet <a href=\"%1\">atslēgas</a> derīgums nav zināms."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "Paraksts ir derīgs un <a href=\"%1\">atslēga</a> ir daļēji uzticama."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "Paraksts ir derīgs un <a href=\"%1\">atslēga</a> ir pilnībā uzticama."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr "Paraksts ir derīgs un <a href=\"%1\">atslēga</a> ir galēji uzticama."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "Paraksts ir derīgs, bet <a href=\"%1\">atslēga</a> nav uzticama."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "Paraksts nav derīgs."

