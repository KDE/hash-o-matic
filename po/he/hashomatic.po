#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr "הקובץ נחתם לפי תקן VS-NfD עם המפתח <a href=\"%1\">%2</a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "הקובץ נחתם באמצעות המפתח <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "פרטי המפתח לא זמינים."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "הקובץ נחתם לפי תקן VS-NfD על ידי %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "הקובץ נחתם על ידי %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "ה<a href=\"%1\">מפתח</a> נשלל."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "תוקף ה<a href=\"%1\">מפתח</a> פג."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr "החתימה תקפה, אך אמינות ה<a href=\"%1\">מפתח</a> לא ידועה."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "החתימה תקפה וה<a href=\"%1\">מפתח</a> אמין באופן גבולי."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "החתימה תקפה וה<a href=\"%1\">מפתח</a> אמין באופן מלא."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr "החתימה תקפה וה<a href=\"%1\">מפתח</a> אמין באופן מוחלט."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "החתימה תקפה, אך ה<a href=\"%1\">מפתח</a> מפוקפק."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "החתימה שגויה"

