#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Fitxategia hori «VS-NfD»rekin etorkor sinatu da <a href=\"%1\">%2</a> gakoa "
"erabiliz."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Fitxategi hori <a href=\"%1\">%2</a> gakoa erabiliz sinatu da."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Gakoaren xehetasuna ez daude erabilgarri."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Fitxategia hori, %1(e)k, «VS-NfD»rekin etorkor sinatu du."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Fitxategi hori %1(e)k sinatu du."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">Gakoa</a> indargabetuta zegoen."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">Gakoa</a> iraungita zegoen."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"Sinadura baliozkoa da, baina <a href=\"%1\">gakoaren</a> baliozkotasuna "
"ezezaguna da."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr ""
"Sinadura baliozkoa da eta <a href=\"%1\">gakoak</a> bazterreko konfiantza du."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr ""
"Sinadura baliozkoa da eta <a href=\"%1\">gakoak</a> konfiantza osoa du."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"Sinadura baliozkoa da, eta <a href=\"%1\">gakoak</a> erabateko konfiantza du."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr ""
"Sinadura baliozkoa da, baina <a href=\"%1\">gakoak</a> ez du konfiantzarik."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "Sinadura baliogabea da."

#: hashhelper.cpp:159
#, kde-format
msgctxt "list separator"
msgid ", "
msgstr ", "

