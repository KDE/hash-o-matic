#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Aquest fitxer s'ha signat conforme amb VS-NfD utilitzant la clau <a href="
"\"%1\">%2</a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Aquest fitxer s'ha signat utilitzant la clau <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Els detalls de la clau no són disponibles."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Aquest fitxer està signat conforme amb VS-NfD per %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Aquest fitxer està signat per %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "S'ha revocat la <a href=\"%1\">clau</a>."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "La <a href=\"%1\">clau</a> ha caducat."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"La signatura és vàlida, però no es coneix la validesa de la <a href="
"\"%1\">clau</a>."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr ""
"La signatura és vàlida, però la <a href=\"%1\">clau</a> és de confiança "
"parcial."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr ""
"La signatura és vàlida i la <a href=\"%1\">clau</a> és de plena confiança."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"La signatura és vàlida i la <a href=\"%1\">clau</a> és de confiança absoluta."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr ""
"La signatura és vàlida, però la <a href=\"%1\">clau</a> no és de confiança."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "La signatura no és vàlida."

#: hashhelper.cpp:159
#, kde-format
msgctxt "list separator"
msgid ", "
msgstr ", "

