#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"इस फ़ाइल को <a href=\"%1\">%2</a> कुंजी का उपयोग करके VS-NfD अनुरूप हस्ताक्षरित किया "
"गया है।"

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr ""
"इस फ़ाइल को कुंजी <a href=\"%1\">%2 का</a> उपयोग करके हस्ताक्षरित किया गया है।"

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "मुख्य विवरण उपलब्ध नहीं हैं।"

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "यह फ़ाइल %1 द्वारा VS-NfD अनुरूप हस्ताक्षरित है।"

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "इस फ़ाइल पर %1 द्वारा हस्ताक्षर किये गये हैं।"

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">चाबी</a> वापस ले ली गई।"

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">चाबी की</a> समय सीमा समाप्त हो चुकी थी।"

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr "हस्ताक्षर वैध है, लेकिन <a href=\"%1\">कुंजी</a> की वैधता अज्ञात है।"

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "हस्ताक्षर वैध है और <a href=\"%1\">कुंजी</a> थोड़ी विश्वसनीय है।"

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "हस्ताक्षर वैध है और <a href=\"%1\">कुंजी</a> पूर्णतः विश्वसनीय है।"

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr "हस्ताक्षर वैध है और <a href=\"%1\">कुंजी</a> अंततः विश्वसनीय है।"

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "हस्ताक्षर वैध है, लेकिन <a href=\"%1\">कुंजी</a> अविश्वसनीय है।"

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "हस्ताक्षर अवैध है."

