#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Bu dosya, <a href=\"%1\">%2</a> anahtarı kullanılarak VS-NfD uyumlu olarak "
"imzalandı."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Bu dosya, <a href=\"%1\">%2</a> anahtarı kullanılarak imzalandı."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Anahtar ayrıntıları kullanılamıyor."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Bu dosya, %1 tarafından VS-NfD uyumlu olarak imzalandı."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Bu dosya, %1 tarafından imzalandı."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">Anahtar</a> yürürlükten kaldırıldı."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">Anahtarın</a> süresi geçti."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"İmza geçerli; ancak <a href=\"%1\">anahtarın</a> geçerliliği bilinmiyor."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr "İmza geçerli; ancak <a href=\"%1\">anahtara</a> pek az güveniliyor."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "İmza geçerli ve <a href=\"%1\">anahtara</a> güveniliyor."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"İmza geçerli ve <a href=\"%1\">anahtara</a> en yüksek düzeyde güveniliyor."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "İmza geçerli; ancak <a href=\"%1\">anahtara</a> güvenilmiyor."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "İmza geçersiz."

