#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Ten plik został podpisany zgodnie z VS-NfD przy użyciu klucza <a href=\"%1\">"
"%2</a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Ten plik został podpisany kluczem <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "Szczegóły klucza nie są dostępne."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Ten plik został podpisany zgodnie z VS-NfD przez %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Plik został podpisany przez %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "<a href=\"%1\">Klucz</a> został odwołany."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "<a href=\"%1\">Klucz</a> został wygasł."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"Podpis jest poprawny, lecz ważność <a href=\"%1\">klucza</a> jest nieznana."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr ""
"Podpis jest poprawny, a <a href=\"%1\">klucz</a> ma marginalny poziom "
"zaufania."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr "Podpis jest poprawny, a <a href=\"%1\">klucz</a> jest w pełni zaufany."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"Podpis jest poprawny, a <a href=\"%1\">klucz</a> ma najwyższy poziom "
"zaufania."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr "Podpis jest poprawny, ale <a href=\"%1\">klucz</a> jest niezaufany."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "Podpis jest nieprawidłowy."

#: hashhelper.cpp:159
#, kde-format
msgctxt "list separator"
msgid ", "
msgstr ", "

