#: hashhelper.cpp:66
#, kde-format
msgctxt "@label"
msgid ""
"This file has been signed VS-NfD compliant using the key <a href=\"%1\">%2</"
"a>."
msgstr ""
"Dit bestand is ondertekend overeenkomstig VS-NfD met de sleutel <a href="
"\"%1\">%2</a>."

#: hashhelper.cpp:71
#, kde-format
msgctxt "@label"
msgid "This file has been signed using the key <a href=\"%1\">%2</a>."
msgstr "Dit bestand is ondertekend met de sleutel <a href=\"%1\">%2</a>."

#: hashhelper.cpp:74
#, kde-format
msgctxt "@label"
msgid "The key details are not available."
msgstr "De details van de sleutel zijn niet beschikbaar."

#: hashhelper.cpp:77
#, kde-format
msgctxt "@label"
msgid "This file has been signed VS-NfD compliant by %1."
msgstr "Dit bestand is ondertekend overeenkomstig VS-NfD door %1."

#: hashhelper.cpp:79
#, kde-format
msgctxt "@label"
msgid "This file has been signed by %1."
msgstr "Dit bestand is ondertekend door %1."

#: hashhelper.cpp:82
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was revoked."
msgstr "De <a href=\"%1\">sleutel</a> is ingetrokken."

#: hashhelper.cpp:85
#, kde-format
msgctxt "@label"
msgid "The <a href=\"%1\">key</a> was expired."
msgstr "De <a href=\"%1\">sleutel</a> is verlopen."

#: hashhelper.cpp:89
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid, but the <a href=\"%1\">key</a>'s validity is unknown."
msgstr ""
"De ondertekening is geldig, maar de geldigheid van de <a href="
"\"%1\">sleutel</a> is onbekend."

#: hashhelper.cpp:91
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is marginally trusted."
msgstr ""
"De ondertekening is geldig en de <a href=\"%1\">sleutel</a> is gedeeltelijk "
"betrouwbaar."

#: hashhelper.cpp:93
#, kde-format
msgctxt "@label"
msgid "The signature is valid and the <a href=\"%1\">key</a> is fully trusted."
msgstr ""
"De ondertekening is geldig en de <a href=\"%1\">sleutel</a> is volledig "
"betrouwbaar."

#: hashhelper.cpp:95
#, kde-format
msgctxt "@label"
msgid ""
"The signature is valid and the <a href=\"%1\">key</a> is ultimately trusted."
msgstr ""
"De ondertekening is geldig en de <a href=\"%1\">sleutel</a> is uiterst "
"betrouwbaar."

#: hashhelper.cpp:97
#, kde-format
msgctxt "@label"
msgid "The signature is valid, but the <a href=\"%1\">key</a> is untrusted."
msgstr ""
"De ondertekening is geldig, maar de <a href=\"%1\">sleutel</a> is "
"onbetrouwbaar."

#: hashhelper.cpp:101
#, kde-format
msgctxt "@label"
msgid "The signature is invalid."
msgstr "De ondertekening is ongeldig."

